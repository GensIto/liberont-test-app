    "typescript": "5.8.3",
    "typescript-eslint": "8.35.0",
    "vite": "7.0.0",
    "wrangler": "4.24.3"
  },
  "scripts": {
    "build": "tsc -b && vite build",
    "cf-typegen": "wrangler types",
    "check": "tsc && vite build && wrangler deploy --dry-run",
    "deploy": "npm run build &&wrangler deploy",
    "dev": "vite",
    "lint": "eslint .",
    "preview": "npm run build && vite preview",
    "db:gen": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply liberont-test-db --local",
    "db:migrate:remote": "wrangler d1 migrations apply liberont-test-db --remote",
    "db:seed:local": "tsx ./src/db/seed.ts",
    "db:seed:remote": "npx wrangler d1 execute liberont-test-db --remote --file=./src/db/seed.sql",
    "db:studio": "drizzle-kit studio --config drizzle.config.local.ts",
    "db:studio:prod": "drizzle-kit studio"
  },
  "type": "module"
}
